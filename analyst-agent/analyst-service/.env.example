# ABOUTME: Example environment variables for the Data Analyst Agent
# ABOUTME: Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Model Configuration
OPENAI_MODEL=gpt-4.1

# Debug Configuration
DEBUG=true
LOG_LEVEL=INFO

# File Upload Configuration
MAX_FILE_SIZE_MB=50
ALLOWED_FILE_TYPES=csv,xlsx,xls

# Output Configuration
OUTPUT_DIR=./results
GRAPH_OUTPUT_DIR=./results

# Frontend Debug Configuration
FRONTEND_DEBUG=true
FRONTEND_LOG_LEVEL=DEBUG
SHOW_ANALYSIS_PROGRESS=true
LOG_REQUEST_IDS=true